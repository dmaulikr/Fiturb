# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Fiturb' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Fiturb
   pod 'FacebookCore'
   pod 'FacebookLogin'
   pod 'FacebookShare'

   # Pods for  FontAwsome
   pod 'Font-Awesome-Swift', '~> 1.6.1'
   pod 'FontAwesome.swift'
   pod 'Cosmos', '~> 9.0'
   
   # Pods for google Places
   pod 'GooglePlaces'

   # Pods for AlamoFireWebServices
   pod 'Alamofire', '~> 4.4'

   # Pods for ObjectMapperSwift
   pod 'ObjectMapper', '~> 2.2'

end
